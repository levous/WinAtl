<h1>Listing class_schedules</h1>

<table>
  <tr>
    <th>Created on</th>
    <th>Updated on</th>
    <th>Course</th>
    <th>Start date</th>
    <th>End date</th>
    <th>Class begins time</th>
    <th>Class ends time</th>
  </tr>

<% @class_schedules.each do |class_schedule| %>
  <tr>
    <td><%=h class_schedule.created_on %></td>
    <td><%=h class_schedule.updated_on %></td>
    <td><%=h class_schedule.course_id %></td>
    <td><%=h class_schedule.start_date %></td>
    <td><%=h class_schedule.end_date %></td>
    <td><%=h class_schedule.class_begins_time %></td>
    <td><%=h class_schedule.class_ends_time %></td>
    <td><%= link_to 'Show', class_schedule %></td>
    <td><%= link_to 'Edit', edit_class_schedule_path(class_schedule) %></td>
    <td><%= link_to 'Destroy', class_schedule, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New class_schedule', new_class_schedule_path %>