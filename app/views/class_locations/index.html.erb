<h1>Listing class_locations</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Address 1</th>
    <th>Address 2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Description</th>
  </tr>

<% @class_locations.each do |class_location| %>
  <tr>
    <td><%=h class_location.name %></td>
    <td><%=h class_location.address_1 %></td>
    <td><%=h class_location.address_2 %></td>
    <td><%=h class_location.city %></td>
    <td><%=h class_location.state %></td>
    <td><%=h class_location.zip %></td>
    <td><%=h class_location.description %></td>
    <td><%= link_to 'Show', class_location %></td>
    <td><%= link_to 'Edit', edit_class_location_path(class_location) %></td>
    <td><%= link_to 'Destroy', class_location, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New class_location', new_class_location_path %>